#lower case to upper case GFG

string to_upper(string str){
     int ln = str.length();
 
    for (int i = 0; i < ln; i++) {
        if (str[i] >= 'a' && str[i] <= 'z')
        
            str[i] = str[i] - 32;
        else if (str[i] >= 'A' && str[i] <= 'Z')
            
            str[i] = str[i] + 32;
    }
    return str;
    
}
